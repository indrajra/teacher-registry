          "$ref": "Common.json#/definitions/Subjects"
        },
        "mainSubjectsTaught": {
          "$id": "#/properties/mainSubjectsTaught",
          "type": "array",
          "items": {
            "$ref": "Common.json#/definitions/Subjects"
          }
        },
        "appointmentYear": {
          "$id": "#/properties/appointmentYear",
          "$ref": "Common.json#/definitions/Year"
        },
        "status": {
          "$id": "#/properties/status",
          "$ref": "Common.json#/definitions/Status"
        }
      }
    }
  },
  "_osConfig": {
